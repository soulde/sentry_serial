<launch>
    <node pkg="sentry_serial" type="sentry_serial_node" name="sentry_serial" output="screen">
        <!-- set a private parameter for the node -->
        <param name="goalTopic" type="string" value="goal"/>
        <param name="subTopic" type="string" value="cmd_vel"/>
        <param name="serialName" type="string" value="/dev/STM32"/>
    </node>

</launch>
